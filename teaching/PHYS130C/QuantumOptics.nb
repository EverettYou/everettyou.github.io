 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["q", "i"], "\[RightArrow]", 
    SubscriptBox[
     OverscriptBox["q", "^"], "i"]}], ",", " ", 
   RowBox[{
    SubscriptBox["p", "i"], "->", 
    SubscriptBox[
     OverscriptBox["p", "^"], "i"]}], ",", "\[IndentingNewLine]", 
   RowBox[{
