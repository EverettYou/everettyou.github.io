Cell[TextData[{
 StyleBox["Canonical quantization",
  FontWeight->"Bold"],
 " is a procedure to transition from ",
 StyleBox["classical mechanics",
  FontSlant->"Italic"],
 " to ",
 StyleBox["quantum mechanics",
  FontSlant->"Italic"],
 ". It is based on the principle of promoting ",
 StyleBox["classical observables",
  FontSlant->"Italic"],
 " (like position and momentum) to ",
 StyleBox["operators",
  FontSlant->"Italic"],
 " acting on a Hilbert space."
}], "TextIndent",ExpressionUUID->"439ce86a-f950-4dc1-82cb-a45c2bcc4711"],

Cell["General Procedure:", "TextConcreteNoIndent",ExpressionUUID->"d4ff6c7d-1d9e-445d-8a80-f4729b9cd199"],

Cell[TextData[{
 "Identify the classical ",
 StyleBox["phase space",
  FontSlant->"Italic"],
 ": a classical system described by generalized ",
 StyleBox["coordinates",
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["q", "i"], TraditionalForm]],ExpressionUUID->
  "c9e4348d-aa84-45cc-b287-5453bea0083c"],
 " and their conjugate ",
 StyleBox["momenta",
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["p", "i"], ":=", 
    RowBox[{
     RowBox[{"\[PartialD]", "L"}], "/", 
     RowBox[{"\[PartialD]", 
      SubscriptBox[
       OverscriptBox["q", "."], "i"]}]}]}], TraditionalForm]],ExpressionUUID->
  "052ab4c6-c65a-4522-a749-e90f4eae92de"],
 "."
}], "Item",ExpressionUUID->"3b9bd6e3-d08c-49c6-b96c-035214630305"],

Cell[TextData[{
 "Promote ",
 StyleBox["classical variables",
  FontSlant->"Italic"],
 " to ",
 StyleBox["quantum operators",
  FontWeight->"Bold"],
 ":"
}], "Item",ExpressionUUID->"9b3a8c8e-8ad9-4d0b-89df-1a4441d2cf64"],

