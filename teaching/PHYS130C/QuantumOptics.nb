Cell["A brute-force yet incorrect answer (by Srinivasa Ramanujan):", "Item",ExpressionUUID->"53a4dc13-abe6-443c-bf16-cec9f5076300"],

Cell[BoxData[
 FormBox[GridBox[{
    {"S", "=", "1", "+", "2", "+", "3", "+", "4", "+", "5", "+", "6", "+", 
     "\[Ellipsis]"},
    {
     RowBox[{"4", "S"}], "=", " ", " ", "4", " ", " ", "+", "8", " ", " ", 
     "+", "12", "+", "\[Ellipsis]"},
    {
     RowBox[{
      RowBox[{"-", "3"}], "S"}], "=", "1", "-", "2", "+", "3", "-", "4", "+", 
     "5", "-", "6", "+", "\[Ellipsis]"},
    {" ", 
     OverscriptBox["=", 
      RowBox[{"x", "=", "1"}]], "1", "-", 
     RowBox[{"2", "x"}], "+", 
     RowBox[{"3", 
      SuperscriptBox["x", "2"]}], "-", 
     RowBox[{"4", 
      SuperscriptBox["x", "3"]}], "+", 
     RowBox[{"5", 
      SuperscriptBox["x", "4"]}], "-", 
     RowBox[{"6", 
      SuperscriptBox["x", "5"]}], "+", "\[Ellipsis]"},
    {" ", "=", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", "+", "x"}], ")"}], 
      RowBox[{"-", "2"}]], "\[SpanFromLeft]", "\[SpanFromLeft]", " ", " ", 
     " ", " ", " ", " ", " ", " ", " ", " "},
    {" ", 
     OverscriptBox["=", 
      RowBox[{"x", "=", "1"}]], 
     RowBox[{"1", "/", "4"}], "\[SpanFromLeft]", " ", " ", " ", " ", " ", " ",
      " ", " ", " ", " ", " "}
   }], TraditionalForm]], "DisplayFormulaNumbered",ExpressionUUID->"57d540b6-\
b6a1-4fa2-99de-d97218c31797"],

