     TemplateBox[{"v"},
      "Ket",
      DisplayFunction->(StyleBox[
        RowBox[{"\[LeftBracketingBar]", #, "\[RightAngleBracket]"}], 
        AutoSpacing -> False]& )]}], "=", 
    RowBox[{
     OverscriptBox["B", "^"], 
     OverscriptBox["A", "^"], 
