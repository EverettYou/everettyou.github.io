  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["O", "^"], 
     OverscriptBox["P", "^"]}], "=", 
    RowBox[{
     OverscriptBox["P", "^"], 
     OverscriptBox["O", "^"]}]}], TraditionalForm]],ExpressionUUID->
  "e49c375f-d149-4ee2-9ecb-4ad39c64c26b"],
