Cell[BoxData[
 RowBox[{"Block", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"dat", ",", "imgs", ",", "pts"}], "}"}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"dat", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"ColorNegate", "/@", 
        RowBox[{"RandomChoice", "[", 
         RowBox[{"#", ",", "3"}], "]"}]}], "&"}], "/@", 
      RowBox[{"GroupBy", "[", 
       RowBox[{
        RowBox[{"ResourceData", "[", 
         RowBox[{"\"\<MNIST\>\"", ",", "\"\<TestData\>\""}], "]"}], ",", 
        RowBox[{"Last", "->", "First"}]}], "]"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"imgs", "=", 
     RowBox[{"Flatten", "@", 
      RowBox[{"Values", "@", "dat"}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"SeedRandom", "[", "449", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"pts", "=", 
     RowBox[{"Flatten", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"#", "+", "\[Epsilon]"}], ",", 
           RowBox[{"{", 
            RowBox[{"\[Epsilon]", ",", 
             RowBox[{"0.2", 
              RowBox[{"RandomVariate", "[", 
               RowBox[{
                RowBox[{"NormalDistribution", "[", "]"}], ",", 
                RowBox[{"{", 
                 RowBox[{"3", ",", "2"}], "}"}]}], "]"}]}]}], "}"}]}], "]"}], 
         "&"}], "/@", 
        RowBox[{"RandomVariate", "[", 
         RowBox[{
          RowBox[{"NormalDistribution", "[", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"10", ",", "2"}], "}"}]}], "]"}]}], ",", "1"}], "]"}]}], 
    ";", "\[IndentingNewLine]", 
    RowBox[{"pts", "=", 
     RowBox[{"pts", "/", 
      RowBox[{"Max", "[", 
       RowBox[{"Norm", "/@", "pts"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Graphics", "[", 
     RowBox[{
      RowBox[{"{", 
