      RowBox[{
       FractionBox[
        RowBox[{"-", "\[ImaginaryI]"}], "2"], "\[Theta]", " ", 
       RowBox[{
        OverscriptBox["n", "^"], "\[CenterDot]", 
        OverscriptBox["\[Sigma]", "\[RightArrow]"]}]}]]}]}], 
