Cell[BoxData[
 RowBox[{"Block", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"d", "=", "512"}], ",", "M", ",", "H", ",", "Es", ",", "\[Psi]s",
      ",", "\[Psi]0", ",", "lgts", ",", "projs", ",", "\[Psi]ts", ",", 
     "\[Rho]s"}], "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"M", "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"#", "+", 
          RowBox[{"ConjugateTranspose", "[", "#", "]"}]}], ")"}], "/", "2"}], 
       "&"}], "[", 
      RowBox[{
       RowBox[{"RandomVariate", "[", 
        RowBox[{
         RowBox[{"NormalDistribution", "[", 
          RowBox[{"0", ",", 
           RowBox[{"1", "/", 
            RowBox[{"Sqrt", "[", "d", "]"}]}]}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"d", ",", "d", ",", "2"}], "}"}]}], "]"}], ".", 
       RowBox[{"{", 
        RowBox[{"1", ",", "\[ImaginaryI]"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"H", "=", 
     RowBox[{"KroneckerProduct", "[", 
      RowBox[{
       RowBox[{"SparseArray", "@", 
        RowBox[{"PauliMatrix", "[", "3", "]"}]}], ",", "M"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"Es", ",", "\[Psi]s"}], "}"}], "=", 
     RowBox[{"Eigensystem", "@", "H"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"\[Psi]0", "=", 
     RowBox[{"Flatten", "[", 
      RowBox[{
       RowBox[{"Normalize", "[", 
        RowBox[{"{", 
         RowBox[{"3", ",", "4"}], "}"}], "]"}], "\[TensorProduct]", 
       RowBox[{"SparseArray", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"1", "\[Rule]", "1"}], "}"}], ",", "d"}], "]"}]}], "]"}]}], 
    ";", "\[IndentingNewLine]", 
    RowBox[{"lgts", "=", 
     RowBox[{"Range", "[", 
      RowBox[{
       RowBox[{"-", "1"}], ",", "2", ",", "0.02"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"projs", "=", 
     RowBox[{
      RowBox[{"Conjugate", "[", "\[Psi]s", "]"}], ".", "\[Psi]0"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"\[Psi]ts", "=", 
     RowBox[{
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"projs", " ", 
         RowBox[{"Exp", "[", 
          RowBox[{
           RowBox[{"-", "\[ImaginaryI]"}], " ", "Es", " ", 
           RowBox[{"10", "^", "lgt"}]}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"lgt", ",", "lgts"}], "}"}]}], "]"}], ".", "\[Psi]s"}]}], 
    ";", "\[IndentingNewLine]", 
    RowBox[{"\[Rho]s", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"#", ".", 
        RowBox[{"ConjugateTranspose", "[", "#", "]"}]}], "&"}], "/@", 
      RowBox[{"ArrayReshape", "[", 
       RowBox[{"\[Psi]ts", ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Length", "@", "lgts"}], ",", "2", ",", "d"}], "}"}]}], 
       "]"}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"ListLinePlot", "[", 
     RowBox[{
      RowBox[{"Thread", "@", 
       RowBox[{"{", 
        RowBox[{"lgts", ",", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"-", 
             RowBox[{"Tr", "[", 
              RowBox[{"#", ".", 
               RowBox[{"MatrixLog", "[", "#", "]"}]}], "]"}]}], "/", 
            RowBox[{"Log", "[", "2", "]"}]}], "&"}], "/@", "\[Rho]s"}]}], 
        "}"}]}], ",", 
      RowBox[{"PlotStyle", "\[Rule]", "Black"}], ",", 
      RowBox[{"PlotLabel", "\[Rule]", 
       RowBox[{"\"\<\\!\\(d=\>\"", "<>", 
        RowBox[{"ToString", "[", "d", "]"}], "<>", "\"\<\\)\>\""}]}], ",", 
      RowBox[{"FrameLabel", "\[Rule]", 
       RowBox[{"{", 
        RowBox[{
        "\"\<\\!\\(lg t\\)\>\"", ",", "\"\<\\!\\(S\\_A(t)\\) [bit]\>\""}], 
        "}"}]}], ",", 
      RowBox[{"PlotRange", "\[Rule]", 
       RowBox[{"{", 
        RowBox[{"0", ",", "1"}], "}"}]}], ",", 
      RowBox[{"ImageSize", "\[Rule]", "200"}]}], "]"}]}]}], "]"}]], "Input",
