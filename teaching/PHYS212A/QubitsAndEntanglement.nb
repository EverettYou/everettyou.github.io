          SuperscriptBox["e", 
           RowBox[{"\[ImaginaryI]", " ", 
            RowBox[{"\[CurlyPhi]", "/", "2"}]}]], "cos", " ", 
          RowBox[{"\[Theta]", "/", "2"}]}]},
        {
         RowBox[{
          SuperscriptBox["e", 
           RowBox[{
            RowBox[{"-", "\[ImaginaryI]"}], " ", 
            RowBox[{"\[CurlyPhi]", "/", "2"}]}]], "sin", " ", 
          RowBox[{"\[Theta]", "/", "2"}]}]}
