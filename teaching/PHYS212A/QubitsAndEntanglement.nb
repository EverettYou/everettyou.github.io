Cell[BoxData[{
 FormBox[
  RowBox[{
   TemplateBox[{"\[Psi]", "\[Psi]"},
    "BraKet",
    DisplayFunction->(
     RowBox[{"\[LeftAngleBracket]", #, "\[VerticalSeparator]", #2, 
       "\[RightAngleBracket]"}]& )], "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SubsuperscriptBox["\[Psi]", "\[UpArrow]", "*"], 
       TemplateBox[{"\[UpArrow]"},
        "Bra",
        DisplayFunction->(
         RowBox[{"\[LeftAngleBracket]", #, "\[RightBracketingBar]"}]& )]}], 
      "+", 
      RowBox[{
       SubsuperscriptBox["\[Psi]", "\[DownArrow]", "*"], 
       TemplateBox[{"\[DownArrow]"},
        "Bra",
        DisplayFunction->(
         RowBox[{"\[LeftAngleBracket]", #, "\[RightBracketingBar]"}]& )]}]}], 
     ")"}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Psi]", "\[UpArrow]"], 
       TemplateBox[{"\[UpArrow]"},
        "Ket",
        DisplayFunction->(
         RowBox[{"\[LeftBracketingBar]", #, "\[RightAngleBracket]"}]& )]}], 
      "+", 
      RowBox[{
       SubscriptBox["\[Psi]", "\[DownArrow]"], 
       TemplateBox[{"\[DownArrow]"},
        "Ket",
        DisplayFunction->(
         RowBox[{"\[LeftBracketingBar]", #, "\[RightAngleBracket]"}]& )]}]}], 
     ")"}]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Psi]", "\[UpArrow]", "*"], 
     SubscriptBox["\[Psi]", "\[UpArrow]"], 
     TemplateBox[{"\[UpArrow]", "\[UpArrow]"},
      "BraKet",
      DisplayFunction->(
       RowBox[{"\[LeftAngleBracket]", #, "\[VerticalSeparator]", #2, 
         "\[RightAngleBracket]"}]& )]}], "+", 
    RowBox[{
     SubsuperscriptBox["\[Psi]", "\[UpArrow]", "*"], 
     SubscriptBox["\[Psi]", "\[DownArrow]"], 
     TemplateBox[{"\[UpArrow]", "\[DownArrow]"},
      "BraKet",
      DisplayFunction->(
       RowBox[{"\[LeftAngleBracket]", #, "\[VerticalSeparator]", #2, 
