    RowBox[{
     SubsuperscriptBox["\[Psi]", "\[DownArrow]", "*"], 
     SubscriptBox["\[Psi]", "\[UpArrow]"], 
     TemplateBox[{"\[DownArrow]", "\[UpArrow]"},
      "BraKet",
      DisplayFunction->(
       RowBox[{"\[LeftAngleBracket]", #, "\[VerticalSeparator]", #2, 
         "\[RightAngleBracket]"}]& )]}], "+", 
    RowBox[{
     SubsuperscriptBox["\[Psi]", "\[DownArrow]", "*"], 
     SubscriptBox["\[Psi]", "\[DownArrow]"], 
