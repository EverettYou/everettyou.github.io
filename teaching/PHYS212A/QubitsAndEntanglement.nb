  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["S", 
      RowBox[{"(", "2", ")"}]], "(", "A", ")"}], ">", "0"}], 
   TraditionalForm]],ExpressionUUID->"bb6d9e17-d2fd-4739-9361-57068b2e25ae"],
