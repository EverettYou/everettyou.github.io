Cell[TextData[{
 "We do not want to specify what ",
 Cell[BoxData[
  FormBox["M", TraditionalForm]],ExpressionUUID->
  "143b42ab-0606-4153-9fd7-1ad974530fd4"],
 " really is \[Implies] ",
 Cell[BoxData[
  FormBox["M", TraditionalForm]],ExpressionUUID->
  "e187cd0d-71c2-4930-b231-00b105cf95c3"],
 " is a ",
 StyleBox["random Hermitian matrix",
  FontWeight->"Bold"],
 ", draw from the ",
 StyleBox["Gaussian unitary ensemble",
  FontWeight->"Bold"],
 ", meaning that the probability density of ",
 Cell[BoxData[
  FormBox["M", TraditionalForm]],ExpressionUUID->
  "40b29fef-d017-46ac-b71b-af0e26778d5c"],
 " is given by"
}], "TextIndent",ExpressionUUID->"555c35b2-44e3-4d47-a769-701539df0058"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"p", "(", "M", ")"}], "\[Proportional]", 
    RowBox[{"exp", "(", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["d", "2"]}], "Tr", " ", 
      SuperscriptBox["M", "2"]}], ")"}]}], ","}], 
  TraditionalForm]], "DisplayFormulaNumbered",ExpressionUUID->"a767e0f7-31e5-\
4096-9378-8e67dc8d95dd"],

