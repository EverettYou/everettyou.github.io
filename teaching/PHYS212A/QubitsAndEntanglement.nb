Cell["\<\
Construct the Hamiltonian and then define the unitary operator\
\>", "TextNoIndent",ExpressionUUID->"28c257ca-6447-459a-9d1e-4d3a5abd5535"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"H", "=", 
   RowBox[{"KroneckerProduct", "[", 
    RowBox[{
     RowBox[{"PauliMatrix", "[", "3", "]"}], ",", "M"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"U", "[", "t_", "]"}], ":=", 
   RowBox[{"MatrixExp", "[", 
    RowBox[{
     RowBox[{"-", "\[ImaginaryI]"}], " ", "H", " ", "t"}], "]"}]}], 
  ";"}]}], "Input",ExpressionUUID->"8b17e1e2-e24b-4e1b-a9a0-119efd0fc2cf"],

Cell["Prepare a initial state", "TextNoIndent",ExpressionUUID->"ebc76ea5-718f-4ceb-bae1-1c7beae77630"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"\[Rho]", "(", "0", ")"}], "=", 
    RowBox[{
     TemplateBox[{
       RowBox[{"\[Psi]", "(", "0", ")"}]},
      "Ket",
      DisplayFunction->(
       RowBox[{"\[LeftBracketingBar]", #, "\[RightAngleBracket]"}]& )], 
     TemplateBox[{
       RowBox[{"\[Psi]", "(", "0", ")"}]},
      "Bra",
      DisplayFunction->(
       RowBox[{"\[LeftAngleBracket]", #, "\[RightBracketingBar]"}]& )]}]}], 
   ",", "\[IndentingNewLine]", 
   RowBox[{
    TemplateBox[{
      RowBox[{"\[Psi]", "(", "0", ")"}]},
     "Ket",
     DisplayFunction->(
      RowBox[{"\[LeftBracketingBar]", #, "\[RightAngleBracket]"}]& )], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"3", 
        TemplateBox[{"\[UpArrow]"},
         "Ket",
         DisplayFunction->(
          RowBox[{"\[LeftBracketingBar]", #, "\[RightAngleBracket]"}]& )]}], 
       "+", 
       RowBox[{"4", 
        TemplateBox[{"\[DownArrow]"},
         "Ket",
         DisplayFunction->(
          RowBox[{"\[LeftBracketingBar]", #, "\[RightAngleBracket]"}]& )]}]}],
       "5"], "\[CircleTimes]", 
     RowBox[{
      TemplateBox[{"1"},
       "Ket",
       DisplayFunction->(
        RowBox[{"\[LeftBracketingBar]", #, "\[RightAngleBracket]"}]& )], 
      "."}]}]}]}], TraditionalForm]], "DisplayFormulaNumbered",ExpressionUUID->\
"c0193828-8a37-4b52-aeb9-91fb5d09c14f"],

Cell[BoxData[
 RowBox[{
  RowBox[{"\[Psi]0", "=", 
   RowBox[{"Flatten", "[", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"3", ",", "4"}], "}"}], "/", "5"}], ")"}], "\[TensorProduct]", 
     RowBox[{"SparseArray", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"1", "\[Rule]", "1"}], "}"}], ",", "d"}], "]"}]}], "]"}]}], 
  ";"}]], "Input",ExpressionUUID->"d2b5c84f-58dd-493a-ac80-2677598b3654"],

