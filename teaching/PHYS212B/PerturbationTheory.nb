Cell[TextData[{
 "The algorithm can be used to ",
 StyleBox["block-diagonalize",
  FontWeight->"Bold"],
 " a Hamiltonian as well. We just need to restrict the choice of the pivot ",
 StyleBox["outside",
  FontSlant->"Italic"],
 " the diagonal blocks."
}], "TextNoIndent",ExpressionUUID->"a2fcbc67-3456-4eb4-892d-c466eb4dec25"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Block", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"L", "=", "5"}], ",", "H0", ",", "mask", ",", "Jacobi", ",", 
     "Hs"}], "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"SeedRandom", "[", "2", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"H0", "=", 
     RowBox[{"Chop", "[", " ", 
      RowBox[{"Hermitianize", "[", 
       RowBox[{
        RowBox[{"RandomVariate", "[", 
         RowBox[{
          RowBox[{"NormalDistribution", "[", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"L", ",", "L", ",", "2"}], "}"}]}], "]"}], ".", 
        RowBox[{"{", 
         RowBox[{"1", ",", "\[ImaginaryI]"}], "}"}]}], "]"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"mask", "=", 
     RowBox[{"Normal", "@", 
      RowBox[{"SparseArray", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{"i_", ",", "j_"}], "}"}], "/;", 
          RowBox[{
           RowBox[{"i", ">", "2"}], "&&", 
           RowBox[{"j", "\[LessEqual]", "2"}]}]}], "\[RuleDelayed]", "1"}], 
        ",", 
        RowBox[{"Dimensions", "[", "H0", "]"}]}], "]"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Jacobi", "[", "H_", "]"}], ":=", 
     RowBox[{"Block", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "m", ",", "n", ",", "r", ",", "\[Phi]", ",", "\[Theta]", ",", "G"}], 
        "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"{", 
           RowBox[{"m", ",", "n"}], "}"}], "}"}], "=", 
         RowBox[{
          RowBox[{
           RowBox[{"Position", "[", 
            RowBox[{"#", ",", 
             RowBox[{"Max", "@", "#"}]}], "]"}], "&"}], "@", 
          RowBox[{"Abs", "[", 
           RowBox[{"mask", " ", "H"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
        
        RowBox[{"r", "=", 
         RowBox[{
          RowBox[{"H", "\[LeftDoubleBracket]", 
           RowBox[{"m", ",", "n"}], "\[RightDoubleBracket]"}], "/", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"H", "\[LeftDoubleBracket]", 
             RowBox[{"n", ",", "n"}], "\[RightDoubleBracket]"}], "-", 
            RowBox[{"H", "\[LeftDoubleBracket]", 
             RowBox[{"m", ",", "m"}], "\[RightDoubleBracket]"}]}], ")"}]}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"\[Phi]", "=", 
         RowBox[{"Arg", "[", "r", "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"\[Theta]", "=", 
         RowBox[{
          RowBox[{"ArcTan", "[", 
           RowBox[{"2", " ", 
            RowBox[{"Abs", "[", "r", "]"}]}], "]"}], "/", "2"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"G", "=", 
         RowBox[{"IdentityMatrix", "@", 
          RowBox[{"Dimensions", "@", "H"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"G", "\[LeftDoubleBracket]", 
          RowBox[{"n", ",", "n"}], "\[RightDoubleBracket]"}], "=", 
         RowBox[{
          RowBox[{"G", "\[LeftDoubleBracket]", 
           RowBox[{"m", ",", "m"}], "\[RightDoubleBracket]"}], "=", 
          RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"G", "\[LeftDoubleBracket]", 
          RowBox[{"m", ",", "n"}], "\[RightDoubleBracket]"}], "=", 
         RowBox[{
          RowBox[{"-", 
           RowBox[{"Sin", "[", "\[Theta]", "]"}]}], 
          RowBox[{"Exp", "[", 
           RowBox[{"\[ImaginaryI]", " ", "\[Phi]"}], "]"}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"G", "\[LeftDoubleBracket]", 
          RowBox[{"n", ",", "m"}], "\[RightDoubleBracket]"}], "=", 
         RowBox[{
          RowBox[{"Sin", "[", "\[Theta]", "]"}], 
          RowBox[{"Exp", "[", 
           RowBox[{
            RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Phi]"}], "]"}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Chop", "[", 
         RowBox[{"G", ".", "H", ".", 
          RowBox[{"ConjugateTranspose", "[", "G", "]"}]}], "]"}]}]}], "]"}]}],
     ";", "\[IndentingNewLine]", 
    RowBox[{"Hs", "=", 
     RowBox[{"NestList", "[", 
      RowBox[{"Jacobi", ",", "H0", ",", "20"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Row", "[", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"ComplexMatrixPlot", "[", 
         RowBox[{"#", ",", 
          RowBox[{"MeshStyle", "\[Rule]", 
           RowBox[{"AbsoluteThickness", "[", "0.7", "]"}]}], ",", 
          RowBox[{"FrameTicks", "\[Rule]", "None"}], ",", 
          RowBox[{"Mesh", "\[Rule]", "All"}], ",", 
          RowBox[{"ImageSize", "\[Rule]", "50"}]}], "]"}], "&"}], "/@", 
       "Hs"}], ",", "\"\<\[RightArrow]\>\""}], "]"}]}]}], 
  "\[IndentingNewLine]", "]"}]], "Input",
 CellLabel->
  "In[601]:=",ExpressionUUID->"cdc50994-6c47-47a6-9415-7608f0417aa4"],

