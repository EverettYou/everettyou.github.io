Cell[BoxData[
 RowBox[{"Block", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"L", "=", "5"}], ",", "H0", ",", "Jacobi", ",", "Hs"}], "}"}], 
   ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"SeedRandom", "[", "0", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"H0", "=", 
     RowBox[{"Chop", "[", " ", 
      RowBox[{"Hermitianize", "[", 
       RowBox[{
        RowBox[{"RandomVariate", "[", 
         RowBox[{
          RowBox[{"NormalDistribution", "[", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"L", ",", "L", ",", "2"}], "}"}]}], "]"}], ".", 
        RowBox[{"{", 
         RowBox[{"1", ",", "\[ImaginaryI]"}], "}"}]}], "]"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Jacobi", "[", "H_", "]"}], ":=", 
     RowBox[{"Block", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "m", ",", "n", ",", "r", ",", "\[Phi]", ",", "\[Theta]", ",", "G"}], 
        "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"{", 
           RowBox[{"m", ",", "n"}], "}"}], "}"}], "=", 
         RowBox[{
          RowBox[{
           RowBox[{"Position", "[", 
            RowBox[{"#", ",", 
             RowBox[{"Max", "@", "#"}]}], "]"}], "&"}], "@", 
          RowBox[{"Abs", "@", 
           RowBox[{"LowerTriangularize", "[", 
            RowBox[{"H", ",", 
             RowBox[{"-", "1"}]}], "]"}]}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"r", "=", 
         RowBox[{
          RowBox[{"H", "\[LeftDoubleBracket]", 
           RowBox[{"m", ",", "n"}], "\[RightDoubleBracket]"}], "/", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"H", "\[LeftDoubleBracket]", 
             RowBox[{"n", ",", "n"}], "\[RightDoubleBracket]"}], "-", 
            RowBox[{"H", "\[LeftDoubleBracket]", 
             RowBox[{"m", ",", "m"}], "\[RightDoubleBracket]"}]}], ")"}]}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"\[Phi]", "=", 
         RowBox[{"Arg", "[", "r", "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"\[Theta]", "=", 
         RowBox[{
          RowBox[{"ArcTan", "[", 
           RowBox[{"2", " ", 
            RowBox[{"Abs", "[", "r", "]"}]}], "]"}], "/", "2"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"G", "=", 
         RowBox[{"IdentityMatrix", "@", 
          RowBox[{"Dimensions", "@", "H"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"G", "\[LeftDoubleBracket]", 
          RowBox[{"n", ",", "n"}], "\[RightDoubleBracket]"}], "=", 
         RowBox[{
          RowBox[{"G", "\[LeftDoubleBracket]", 
           RowBox[{"m", ",", "m"}], "\[RightDoubleBracket]"}], "=", 
          RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"G", "\[LeftDoubleBracket]", 
          RowBox[{"m", ",", "n"}], "\[RightDoubleBracket]"}], "=", 
         RowBox[{
          RowBox[{"-", 
           RowBox[{"Sin", "[", "\[Theta]", "]"}]}], 
          RowBox[{"Exp", "[", 
           RowBox[{"\[ImaginaryI]", " ", "\[Phi]"}], "]"}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"G", "\[LeftDoubleBracket]", 
          RowBox[{"n", ",", "m"}], "\[RightDoubleBracket]"}], "=", 
         RowBox[{
          RowBox[{"Sin", "[", "\[Theta]", "]"}], 
          RowBox[{"Exp", "[", 
           RowBox[{
            RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Phi]"}], "]"}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Chop", "[", 
         RowBox[{"G", ".", "H", ".", 
          RowBox[{"ConjugateTranspose", "[", "G", "]"}]}], "]"}]}]}], 
      "\[IndentingNewLine]", "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Hs", "=", 
     RowBox[{"NestList", "[", 
      RowBox[{"Jacobi", ",", "H0", ",", "20"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Column", "@", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Row", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"ComplexMatrixPlot", "[", 
            RowBox[{"#", ",", 
             RowBox[{"FrameTicks", "\[Rule]", "None"}], ",", 
             RowBox[{"Mesh", "\[Rule]", "All"}], ",", 
             RowBox[{"MeshStyle", "\[Rule]", 
              RowBox[{"AbsoluteThickness", "[", "0.7", "]"}]}], ",", 
             RowBox[{"ImageSize", "\[Rule]", "50"}]}], "]"}], "&"}], "/@", 
          "Hs"}], ",", "\"\<\[RightArrow]\>\""}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"Grid", "[", 
        RowBox[{"{", 
