Cell["Denote the diagonal and off-diagonal elements as", "TextIndent",ExpressionUUID->"6859f8ca-c0d5-49c7-8974-eec5df6dfc24"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["E", "n"], "=", 
    SubscriptBox["H", 
     RowBox[{"n", "\[VeryThinSpace]", "n"}]]}], ",", "\[IndentingNewLine]", 
   RowBox[{
    SubscriptBox["V", 
     RowBox[{"m", "\[VeryThinSpace]", "n"}]], "=", 
    RowBox[{
     SubscriptBox["H", 
      RowBox[{"m", "\[VeryThinSpace]", "n"}]], " ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"for", " ", "m"}], "\[NotEqual]", "n"}], ")"}], "."}]}]}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",ExpressionUUID->"178f121a-12bb-\
4307-aa2f-a1adaf7e0a5b"],

Cell[TextData[{
 "In the limit ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["V", 
     RowBox[{"m", "\[VeryThinSpace]", "n"}]], "\[RightArrow]", "0"}], 
   TraditionalForm]],ExpressionUUID->"c365fbf5-946e-4e62-a197-b51553c97d6b"],
 ", the angle ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Theta]", "\[Rule]", "0"}], TraditionalForm]],ExpressionUUID->
  "76b16703-e028-4517-9385-52c2a01f88d1"],
 " according to Eq. (",
 
