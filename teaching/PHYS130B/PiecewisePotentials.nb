Cell[TextData[{
 StyleBox["Localization",
  FontWeight->"Bold"],
 ": A wave become ",
 StyleBox["localized",
  FontWeight->"Bold"],
 " if it ",
 StyleBox["decays exponentially",
  FontSlant->"Italic"],
 " away from some local region, rather than spreading throughout the space. "
}], "Item",ExpressionUUID->"0d5c8770-1026-49f4-a34c-bfa67064e66f"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"\[LeftBracketingBar]", 
     RowBox[{"\[Psi]", "(", "x", ")"}], "\[RightBracketingBar]"}], "\[Tilde]", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"\[LeftBracketingBar]", 
        RowBox[{"x", "-", 
         SubscriptBox["x", "0"]}], "\[RightBracketingBar]"}]}], "/", 
      "\[Xi]"}]]}], ","}], TraditionalForm]], "DisplayFormulaNumbered",Express\
ionUUID->"51814851-a271-4c22-b304-68374e195010"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["\[Xi]", TraditionalForm]],ExpressionUUID->
  "0cd6f5a3-ba4e-41ac-bb07-b429d619056d"],
 " is the ",
 StyleBox["localization length",
  FontWeight->"Bold"],
 "."
}], "ItemParagraph",ExpressionUUID->"4e2c9996-014b-44f5-afc7-1ad143ae3626"],

Cell[CellGroupData[{

Cell[TextData[{
 StyleBox["Disorder",
  FontWeight->"Bold"],
 " and ",
 StyleBox["Anderson localization",
  FontWeight->"Bold"],
 ": in 1D, even ",
 StyleBox["weak",
  FontSlant->"Italic"],
 " disorder can dramatically affect wave propagation,"
}], "Item",ExpressionUUID->"34c4f52c-0f6d-436f-bb29-7fbdb9e1ec04"],

Cell[TextData[{
 "Every potential ",
 StyleBox["scatters",
  FontSlant->"Italic"],
 " part of the wave,"
}], "Subitem",ExpressionUUID->"555630d9-86f3-448e-ad67-9e51e2c2dfeb"],

Cell[TextData[{
 StyleBox["Randomness",
  FontSlant->"Italic"],
 " causes ",
 StyleBox["phases",
  FontWeight->"Bold"],
 " to ",
 StyleBox["misalign",
  FontSlant->"Italic"],
 ", suppressing constructive interference,"
}], "Subitem",ExpressionUUID->"1d0113e8-6ad3-4d7f-b2d5-8f6e23e8b8b9"],

Cell[TextData[{
 "Over long distance, ",
 StyleBox["scattered",
  FontSlant->"Italic"],
 " waves ",
 StyleBox["interfere destructively",
  FontWeight->"Bold"],
 " with the ",
 StyleBox["forward propagating",
  FontSlant->"Italic"],
 " wave, effectively ",
 StyleBox["trapping",
  FontSlant->"Italic"],
 " the wave."
}], "Subitem",ExpressionUUID->"8cf2fe61-4b52-4fa2-ac43-60aedebfede4"]
