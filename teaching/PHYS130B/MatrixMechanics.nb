  RowBox[{
   RowBox[{"{", 
    RowBox[{"dat", ",", "imgs", ",", "pts"}], "}"}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"dat", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"ColorNegate", "/@", 
        RowBox[{"RandomChoice", "[", 
         RowBox[{"#", ",", "3"}], "]"}]}], "&"}], "/@", 
      RowBox[{"GroupBy", "[", 
       RowBox[{
        RowBox[{"ResourceData", "[", 
         RowBox[{"\"\<MNIST\>\"", ",", "\"\<TestData\>\""}], "]"}], ",", 
        RowBox[{"Last", "->", "First"}]}], "]"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"imgs", "=", 
     RowBox[{"Flatten", "@", 
      RowBox[{"Values", "@", "dat"}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"SeedRandom", "[", "449", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"pts", "=", 
     RowBox[{"Flatten", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"Table", "[", 
