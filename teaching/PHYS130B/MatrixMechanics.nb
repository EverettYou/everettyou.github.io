           RowBox[{"#", "+", "\[Epsilon]"}], ",", 
           RowBox[{"{", 
            RowBox[{"\[Epsilon]", ",", 
             RowBox[{"0.2", 
              RowBox[{"RandomVariate", "[", 
               RowBox[{
                RowBox[{"NormalDistribution", "[", "]"}], ",", 
                RowBox[{"{", 
                 RowBox[{"3", ",", "2"}], "}"}]}], "]"}]}]}], "}"}]}], "]"}], 
