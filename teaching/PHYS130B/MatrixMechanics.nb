Cell[CellGroupData[{
Cell["130B Quantum Physics", "Title",ExpressionUUID->"35a6bf00-3930-4d56-b8e4-9d2b661213ca"],

Cell["Part I. Matrix Mechanics", "Subtitle",ExpressionUUID->"64700700-1aa2-4ced-b903-f64c0e2288a4"],

Cell[CellGroupData[{

Cell["Introduction", "Section",ExpressionUUID->"9012186a-6a56-4b95-9ee5-8f5e745be1bf"],

Cell[CellGroupData[{

Cell["Everything Is a Vector", "Subsection",ExpressionUUID->"faebc73d-f664-4d19-b0c1-b50b9e01b981"],

Cell[CellGroupData[{

Cell["What Is Quantum Mechanics?", "Subsubsection",ExpressionUUID->"8ebe045d-1e67-4317-ba2d-481f8a3e49cb"],

Cell[TextData[{
 StyleBox["Quantum mechanics",
  FontWeight->"Bold"],
 " is a ",
 StyleBox["physics theory",
  FontSlant->"Italic"],
 " that describes the behavior of ",
 StyleBox["quantum systems",
  FontSlant->"Italic"],
 " (microscopic particles, strings, qubits ...)."
}], "TextIndent",ExpressionUUID->"85ffcdb9-247d-48cd-92da-27999ba22080"],

Cell[TextData[{
 "What does ",
 StyleBox["physics theory",
  FontWeight->"Bold"],
 " do in general?"
}], "TextIndent",ExpressionUUID->"9c9215df-367f-4eb8-85e5-d75621a0ddb9"],

Cell[TextData[{
 "Describe the ",
 StyleBox["state",
  FontWeight->"Bold"],
 " of the system: a set of ",
 StyleBox["variables",
  FontSlant->"Italic"],
 " encoding the relevant ",
 StyleBox["information",
  FontSlant->"Italic"],
 " of the system."
}], "Item",ExpressionUUID->"2fb80470-adf1-4916-b537-91a7e3b49062"],

Cell[TextData[{
 "Predict (i) the ",
 StyleBox["observables",
  FontWeight->"Bold"],
 " (measurement outcomes) and (ii) their ",
 StyleBox["dynamics",
  FontWeight->"Bold"],
 " (time evolution)."
}], "Item",ExpressionUUID->"513c06a6-c4e0-4b77-a1f1-b970a6671714"],

Cell[TextData[{
 "Physics theory is about encoding the physical reality in the form of ",
 StyleBox["information",
  FontWeight->"Bold"],
 " and making predictions about the reality based on such information."
}], "TextNoIndent",ExpressionUUID->"c6349731-bf74-4a75-a777-d3ae87c07fee"]
}, Open  ]],

Cell[CellGroupData[{

Cell["How to Encode Information", "Subsubsection",ExpressionUUID->"5a9174f6-2c21-435c-95aa-dd853191a737"],

Cell[TextData[{
 StyleBox["State variables",
  FontWeight->"Bold"],
 " encode the information about the system. They are ",
 StyleBox["inferred",
  FontSlant->"Italic"],
 " from observations."
}], "TextIndent",ExpressionUUID->"740f0fee-b903-498c-8e46-1f390c45fdd9"],

Cell["State variables may not have \[OpenCurlyDoubleQuote]physical meaning\
\[CloseCurlyDoubleQuote].", "Item",ExpressionUUID->"58b2df38-f9d3-41aa-a2f4-\
460c477f3f5f"],

