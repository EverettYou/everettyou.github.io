Cell[TextData[{
 "The ",
 StyleBox["angular momentum",
  FontWeight->"Bold"],
 " of a quantum system (in 3D space) is described by a set of ",
 StyleBox["three Hermitian operators",
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    OverscriptBox["J", "^"], "1"], TraditionalForm]], "TextNoIndent",
  ExpressionUUID->"ae45e69c-408a-4ed7-9ffc-dc3dcf9317f5"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    OverscriptBox["J", "^"], "2"], TraditionalForm]],ExpressionUUID->
  "9a5fec33-4524-407b-9cf3-12a77716ecba"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    OverscriptBox["J", "^"], "3"], TraditionalForm]],ExpressionUUID->
  "ff2899ae-4095-4837-8811-e6255f314d13"],
 ", jointly written as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox[
     OverscriptBox[
      StyleBox["J",
       FontWeight->"Bold"], 
      StyleBox["^",
       FontWeight->"Plain"]],
     FontWeight->"Bold"], "=", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox[
       OverscriptBox["J", "^"], "1"], ",", 
      SubscriptBox[
       OverscriptBox["J", "^"], "2"], ",", 
      SubscriptBox[
       OverscriptBox["J", "^"], "3"]}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"3e9530e6-bbb9-47c8-90a8-27dc6ec4b095"],
 ", satisfying the following commutation relation"
}], "TextIndent",ExpressionUUID->"e396d74c-a229-4263-991b-de92df59fe41"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"[", 
