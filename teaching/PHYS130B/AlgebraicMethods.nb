     SuperscriptBox[
      RowBox[{"\[Pi]", "(", 
       RowBox[{
        RowBox[{
         RowBox[{"1", "/", "2"}], "\[CircleTimes]", "1"}], "\[Rule]", 
        RowBox[{"3", "/", "2"}]}], ")"}], "\[Dagger]"], 
