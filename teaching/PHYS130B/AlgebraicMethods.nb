Cell[TextData[{
 "The shape of the orbital are often indicated by the ",
 StyleBox["harmonic polynomials",
  FontWeight->"Bold"],
 " (e.g. ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", " ", "y"}], TraditionalForm]],ExpressionUUID->
  "61a4b5df-c46a-444c-a303-fe65ed2f5bbb"],
