      RowBox[{
       RowBox[{"m", "\[VeryThinSpace]", "\[Alpha]"}], ",", 
       RowBox[{"n", "\[VeryThinSpace]", "\[Beta]"}]}]], 
     RowBox[{
      TemplateBox[{
        RowBox[{"n", "\[VeryThinSpace]", "\[Beta]"}]},
       "Bra",
       DisplayFunction->(
        RowBox[{"\[LeftAngleBracket]", #, "\[RightBracketingBar]"}]& )], 
      "."}]}]}]}], TraditionalForm]], "DisplayFormulaFramed",ExpressionUUID->\
"54cdeacd-3129-4f32-81b7-0866a351d531"],

