          RowBox[{"Sin", "[", "\[Theta]", "]"}], 
          RowBox[{"Exp", "[", 
           RowBox[{
            RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Phi]"}], "]"}]}]}], ";", 
