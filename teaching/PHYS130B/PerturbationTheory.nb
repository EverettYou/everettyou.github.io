 Cell[BoxData[
  FormBox[
   SubscriptBox[
    TemplateBox[{
      RowBox[{"\[Psi]", "(", "0", ")"}]},
     "Ket",
     DisplayFunction->(
      RowBox[{"\[LeftBracketingBar]", #, "\[RightAngleBracket]"}]& )], 
    "\[ScriptCapitalS]"], TraditionalForm]],ExpressionUUID->
  "af3fcd44-ec50-48a2-96a0-070e955fb28e"],
 " to obtain the final state ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    TemplateBox[{
      RowBox[{"\[Psi]", "(", "t", ")"}]},
     "Ket",
     DisplayFunction->(
      RowBox[{"\[LeftBracketingBar]", #, "\[RightAngleBracket]"}]& )], 
    "\[ScriptCapitalS]"], TraditionalForm]],ExpressionUUID->
  "166e6fd8-4ffb-4a6c-9049-f88b934bc3be"],
