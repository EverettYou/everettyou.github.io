 FormBox[
  RowBox[{
   OverscriptBox["V", "^"], "=", 
   RowBox[{
    UnderscriptBox["\[Sum]", 
     RowBox[{
      RowBox[{"m", "\[VeryThinSpace]", "\[Alpha]"}], ",", 
      RowBox[{"n", "\[VeryThinSpace]", "\[Beta]"}]}]], 
    RowBox[{
     TemplateBox[{
       RowBox[{"m", "\[VeryThinSpace]", "\[Alpha]"}]},
      "Ket",
      DisplayFunction->(
       RowBox[{"\[LeftBracketingBar]", #, "\[RightAngleBracket]"}]& )], 
