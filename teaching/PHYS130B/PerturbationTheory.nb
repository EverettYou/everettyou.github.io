 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["U", "^"], "(", "t", ")"}], TraditionalForm]],
  ExpressionUUID->"7b822db6-64f9-4565-9e9a-b2373d3180c2"],
 ", called the ",
 StyleBox["time-evolution operator",
  FontWeight->"Bold"],
