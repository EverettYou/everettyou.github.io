     RowBox[{"L", "=", "5"}], ",", "Jacobi", ",", "mask", ",", "K", ",", "V", 
     ",", "H0", ",", "H1", ",", "H2", ",", "cmp"}], "}"}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"SeedRandom", "[", "2", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"mask", "=", 
     RowBox[{"Normal", "@", 
      RowBox[{"SparseArray", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{"i_", ",", "j_"}], "}"}], "/;", 
          RowBox[{
           RowBox[{"i", ">", "2"}], "&&", 
           RowBox[{"j", "\[LessEqual]", "2"}]}]}], "\[RuleDelayed]", "1"}], 
        ",", 
        RowBox[{"{", 
         RowBox[{"L", ",", "L"}], "}"}]}], "]"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Jacobi", "[", "H_", "]"}], ":=", 
     RowBox[{"Block", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "m", ",", "n", ",", "r", ",", "\[Phi]", ",", "\[Theta]", ",", "G"}], 
        "}"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"m", ",", "n"}], "}"}], "=", 
         RowBox[{
          RowBox[{
           RowBox[{"First", "@", 
            RowBox[{"Position", "[", 
             RowBox[{"#", ",", 
              RowBox[{"Max", "@", "#"}]}], "]"}]}], "&"}], "@", 
          RowBox[{"Abs", "[", 
           RowBox[{"mask", " ", "H"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
        
        RowBox[{"r", "=", 
         RowBox[{
          RowBox[{"H", "\[LeftDoubleBracket]", 
           RowBox[{"m", ",", "n"}], "\[RightDoubleBracket]"}], "/", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"H", "\[LeftDoubleBracket]", 
             RowBox[{"n", ",", "n"}], "\[RightDoubleBracket]"}], "-", 
            RowBox[{"H", "\[LeftDoubleBracket]", 
             RowBox[{"m", ",", "m"}], "\[RightDoubleBracket]"}]}], ")"}]}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"\[Phi]", "=", 
         RowBox[{"Arg", "[", "r", "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"\[Theta]", "=", 
         RowBox[{
          RowBox[{"ArcTan", "[", 
           RowBox[{"2", " ", 
            RowBox[{"Abs", "[", "r", "]"}]}], "]"}], "/", "2"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"G", "=", 
         RowBox[{"IdentityMatrix", "@", 
          RowBox[{"Dimensions", "@", "H"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"G", "\[LeftDoubleBracket]", 
