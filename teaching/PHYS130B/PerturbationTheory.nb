Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox[
     OverscriptBox["U", "^"], "0"], "(", "t", ")"}], "=", 
   RowBox[{
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], " ", 
      SubscriptBox[
       OverscriptBox["H", "^"], "0"], "t"}]], "=", 
    RowBox[{
     UnderscriptBox["\[Sum]", "n"], 
     RowBox[{
      TemplateBox[{"n"},
       "Ket",
       DisplayFunction->(
        RowBox[{"\[LeftBracketingBar]", #, "\[RightAngleBracket]"}]& )], 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", 
        SubscriptBox["E", "n"], "t"}]], 
      RowBox[{
       TemplateBox[{"n"},
        "Bra",
