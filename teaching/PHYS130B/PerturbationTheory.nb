Cell[TextData[{
 "such that the eigenenergy ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["E", "n"], TraditionalForm]],ExpressionUUID->
  "e603c498-dbd4-4fa4-ab15-2a18f30468ed"],
 " only depends on ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "b671322f-96b1-4587-a692-5ca4aa8ad6c7"],
 "."
}], "TextNoIndent",ExpressionUUID->"0dd39edf-fe8a-485e-b872-b2dc321e81dd"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   TemplateBox[{
     RowBox[{"n", "\[VeryThinSpace]", "\[Alpha]"}]},
    "Ket",
    DisplayFunction->(
     RowBox[{"\[LeftBracketingBar]", #, "\[RightAngleBracket]"}]& )], 
   TraditionalForm]],ExpressionUUID->"3d914535-7c90-452e-b335-b897fc65ea5e"],
 " form a set of ",
 StyleBox["orthonormal",
  FontSlant->"Italic"],
 " basis: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{
      RowBox[{"m", "\[VeryThinSpace]", "\[Alpha]"}], 
      RowBox[{"n", "\[VeryThinSpace]", "\[Beta]"}]},
     "BraKet",
     DisplayFunction->(
      RowBox[{"\[LeftAngleBracket]", #, "\[VerticalSeparator]", #2, 
        "\[RightAngleBracket]"}]& )], "=", 
    RowBox[{
     SubscriptBox["\[Delta]", 
      RowBox[{"m", "\[VeryThinSpace]", "n"}]], 
     SubscriptBox["\[Delta]", 
      RowBox[{"\[Alpha]", "\[VeryThinSpace]", "\[Beta]"}]]}]}], 
   TraditionalForm]],ExpressionUUID->"47609d7f-7cc6-4a68-bbb8-74def3573895"],
 "."
}], "Item",ExpressionUUID->"30584c35-bcbf-4282-8f64-1c6aa041a412"],

Cell[TextData[{
 "The ",
 StyleBox["perturbation operator",
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]],ExpressionUUID->
  "e91e5b7a-1132-4b4e-892e-973cf06a9922"],
 " can be represented in this basis:"
}], "Item",ExpressionUUID->"f2c5550e-dd16-4cc4-8fad-1c0d643ded0e"],

