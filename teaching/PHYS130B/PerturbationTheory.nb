Cell[TextData[{
 "Consider the perturbation is gradually turn on following an exponential \
grow from the infinite past (and switch off after ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "=", "0"}], TraditionalForm]],ExpressionUUID->
  "d6e0b073-5db2-4b85-b2d7-f9b6f293710c"],
 ")"
}], "TextIndent",ExpressionUUID->"3817fef9-c052-44da-baeb-a47f52dcb446"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    OverscriptBox["V", "^"], "(", "t", ")"}], "=", 
   RowBox[{
    TagBox[GridBox[{
       {"\[Piecewise]", GridBox[{
          {
           RowBox[{
            OverscriptBox["V", "^"], " ", 
            SuperscriptBox["\[ExponentialE]", 
             RowBox[{"t", "/", "\[Tau]"}]]}], 
