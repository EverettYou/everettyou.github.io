Cell[TextData[{
 StyleBox["Solution:", "TextConcreteNoIndent"],
 " Similarly, to define a meaningful derivative for the wavefunction, we have \
to introduce a ",
 StyleBox["gauge connection ",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["A", "\[Mu]"], "(", "x", ")"}], TraditionalForm]],
  ExpressionUUID->"fc3a7b17-f625-448d-9292-0a8aef1f7a88"],
 " to keep track of the ",
 StyleBox["phase rotation",
  FontSlant->"Italic"],
 " needed to transport ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Psi]", "(", 
    RowBox[{
     RowBox[{"x", "+", "\[Delta]"}], "\[InvisibleComma]", "x"}], ")"}], 
   TraditionalForm]],ExpressionUUID->"a2233c52-cf9c-47c1-984a-7e2199041d6b"],
 " back to the point ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "40a18ed2-4757-4aa5-a4e4-68909c2c7bce"],
 " for comparison, for every point ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "36684b25-f959-4a1c-a01e-3947945ae63b"],
 " along any direction ",
 Cell[BoxData[
  FormBox["\[Mu]", TraditionalForm]],ExpressionUUID->
  "e0bc0197-6e7c-47a1-b1d4-e5aebe8061b1"],
 "."
}], "Item",ExpressionUUID->"b2427bb4-c0f1-45f1-ad73-65ffaafab02e"],

Cell[TextData[{
 "This allows us to (re)define the ",
 StyleBox["covariant derivative",
  FontWeight->"Bold"],
 ":"
}], "TextNoIndent",ExpressionUUID->"d4477bae-fbe6-4fc5-a246-cc6704a3a4a7"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["D", "\[Mu]"], 
    RowBox[{"\[Psi]", "(", "x", ")"}]}], "=", 
   RowBox[{
    UnderscriptBox["lim", 
     RowBox[{"\[Delta]", "\[InvisibleComma]", 
      RowBox[{"x", "->", "0"}]}]], 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{
          RowBox[{
           RowBox[{"-", 
            FractionBox["\[ImaginaryI]", "\[HBar]"]}], 
           RowBox[{
            SubscriptBox["A", "\[Nu]"], "(", "x", ")"}], "\[Delta]"}], 
          "\[InvisibleComma]", 
          SuperscriptBox["x", "\[Nu]"]}]], 
        RowBox[{"\[Psi]", "(", 
         RowBox[{
          RowBox[{"x", "+", "\[Delta]"}], "\[InvisibleComma]", "x"}], ")"}]}],
        "-", 
       RowBox[{"\[Psi]", "(", "x", ")"}]}], 
      RowBox[{"\[Delta]", "\[InvisibleComma]", 
       SuperscriptBox["x", "\[Mu]"]}]], "."}]}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",
 CellTags->
  "xref:84561",ExpressionUUID->"4d4f0202-d039-45ed-9808-d92c791d1b6e"],

Cell[TextData[{
 StyleBox["Interpretation:", "TextConcreteNoIndent"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Psi]", "(", 
    RowBox[{
     RowBox[{"x", "+", "\[Delta]"}], "\[InvisibleComma]", "x"}], ")"}], 
   TraditionalForm]],ExpressionUUID->"0a9975a6-eb70-4ed5-9e21-ac3e62976738"],
 " has accumulated a Berry phase of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"exp", "(", 
    RowBox[{
     RowBox[{
      FractionBox["\[ImaginaryI]", "\[HBar]"], 
      RowBox[{
       SubscriptBox["A", "\[Nu]"], "(", "x", ")"}], "\[Delta]"}], 
     "\[InvisibleComma]", 
     SuperscriptBox["x", "\[Nu]"]}], ")"}], TraditionalForm]],ExpressionUUID->
  "c789131a-e28c-4d84-8d17-c92207a3ac8e"],
 " compare to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Psi]", "(", "x", ")"}], TraditionalForm]],ExpressionUUID->
  "335359e2-881b-4368-b583-195732b7a55e"],
 " as the particle travels adiabatically. So when pulling ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Psi]", "(", 
    RowBox[{
     RowBox[{"x", "+", "\[Delta]"}], "\[InvisibleComma]", "x"}], ")"}], 
   TraditionalForm]],ExpressionUUID->"4ea2fdc7-6106-4ea1-b536-d373c3c91280"],
 " back to the point ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "c353bae3-e61f-4b6c-bd21-147047cbb1c0"],
 ", this phase should be compensated by the opposite phase factor ",
 Cell[BoxData[
  FormBox[
   RowBox[{"exp", "(", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       FractionBox["\[ImaginaryI]", "\[HBar]"]}], 
      RowBox[{
       SubscriptBox["A", "\[Nu]"], "(", "x", ")"}], "\[Delta]"}], 
     "\[InvisibleComma]", 
     SuperscriptBox["x", "\[Nu]"]}], ")"}], TraditionalForm]],ExpressionUUID->
  "ac8cee1a-3b9f-4580-a3f6-347c08e4ba59"],
 " before comparing."
}], "Item",ExpressionUUID->"3a4af23f-3557-466e-bf92-96feb86fdfa6"],

Cell["\<\
Expressed in terms of the usual partial derivative modified by the gauge \
connection,\
\>", "Item",ExpressionUUID->"94661ccc-e894-4ce1-8c07-09bde4fbeb18"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["D", "\[Mu]"], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[PartialD]", "\[Mu]"], 
      RowBox[{"-", 
       FractionBox["\[ImaginaryI]", "\[HBar]"]}]}], 
     RowBox[{
      SubscriptBox["A", "\[Mu]"], "(", "x", ")"}]}]}], ","}], 
  TraditionalForm]], "DisplayFormulaFramed",
 CellTags->
  "xref:84899",ExpressionUUID->"c1d951f0-8e65-42d5-a2a9-4bc832b2fb65"],

Cell[TextData[{
 "which exactly reproduces Eq.\[NonBreakingSpace](",
 
 CounterBox["DisplayFormulaNumbered", "xref:4354"],
 ")."
}], "ItemParagraph",ExpressionUUID->"53095981-0bfc-4687-83cc-0fddac91cc8d"],

