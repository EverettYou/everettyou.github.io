Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     SubscriptBox[
      StyleBox["A",
       FontWeight->"Bold"], "N"], "(", 
     RowBox[{"\[Theta]", ",", "\[CurlyPhi]"}], ")"}], "=", 
    RowBox[{
     FractionBox["\[HBar]", "2"], 
     FractionBox[
      RowBox[{
       RowBox[{"cos", " ", "\[Theta]"}], "-", "1"}], 
      RowBox[{"sin", " ", "\[Theta]"}]], 
     SubscriptBox[
      StyleBox["e",
       FontWeight->"Bold"], "\[CurlyPhi]"]}]}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     SubsuperscriptBox["Y", 
      RowBox[{
       RowBox[{"1", "/", "2"}], ",", 
       RowBox[{"1", "/", "2"}], ",", 
       RowBox[{
        RowBox[{"+", "1"}], "/", "2"}]}], "N"], "(", 
     RowBox[{"\[Theta]", ",", "\[CurlyPhi]"}], ")"}], "=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox[
       RowBox[{"4", "\[Pi]"}]]], 
     SqrtBox[
      RowBox[{"1", "+", 
       RowBox[{"cos", " ", "\[Theta]"}]}]]}]}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     SubsuperscriptBox["Y", 
      RowBox[{
       RowBox[{"1", "/", "2"}], ",", 
       RowBox[{"1", "/", "2"}], ",", 
       RowBox[{
        RowBox[{"-", "1"}], "/", "2"}]}], "N"], "(", 
     RowBox[{"\[Theta]", ",", "\[CurlyPhi]"}], ")"}], "=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox[
       RowBox[{"4", "\[Pi]"}]]], 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], " ", "\[CurlyPhi]"}]], 
     RowBox[{
      SqrtBox[
       RowBox[{"1", "-", 
        RowBox[{"cos", " ", "\[Theta]"}]}]], "."}]}]}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",ExpressionUUID->"34b1b6f6-48f1-\
